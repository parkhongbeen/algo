    "def solution(n):\n",
    "    answer = ''\n",
    "    for i in range(1, n+1):\n",
    "        if i % 2 == 1:\n",
    "            answer += '수'\n",
    "        if i % 2 == 0:\n",
    "            answer += '박'\n",
    "    return answer\n",
    "\n",
    "def solution2(n):\n",
    "    return ('수박'*n)[0:n]"
