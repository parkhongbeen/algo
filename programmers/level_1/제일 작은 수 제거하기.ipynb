    "# 2번째 방법 sorted(revers=True)\n",
    "\n",
    "def solution(arr):\n",
    "    arr.remove(min(arr))\n",
    "    if not arr:\n",
    "        arr.insert(0, -1)\n",
    "        \n",
    "    return arr"
