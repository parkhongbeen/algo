   "source": [
    "# 에라토스테네스의 체의 원리를 사용해야 함\n",
    "def solution(n):\n",
    "    answer = set(range(2, n+1))\n",
    "    for i in range(2, n+1):\n",
    "        if i in answer:\n",
    "            answer -= set(range(2*i, n+1, i))\n",
    "    return len(answer)"
   ]
