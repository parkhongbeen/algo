    "def solution(array, commands):\n",
    "    answer = []\n",
    "    for i,j,k in commands:\n",
    "        temp = array[i-1:j]\n",
    "        temp.sort()\n",
    "        answer.append(temp[k-1])\n",
    "    return answer"
   ]
