    "N = int(input())\n",
    "A = list(map(int, input().split()))\n",
    "\n",
    "DP = [1 for _ in range(N)] # DP[i]: 1번째 수까지s 최장 길이\n",
    "\n",
    "for i in range(1, N):\n",
    "    for j in range(i):\n",
    "        if A[i] > A[j]:\n",
    "            DP[i] = max(DP[i], DP[j]+1)\n",
    "            \n",
    "print(max(DP))"
