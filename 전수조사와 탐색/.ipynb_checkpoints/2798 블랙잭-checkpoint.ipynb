   "source": [
    "N, M = map(int, input().split())\n",
    "lst = list(map(int, input().split()))\n",
    "\n",
    "answer = 0\n",
    "for i in range(N):\n",
    "    for j in range(i+1, N):\n",
    "        for k in range(j+1, N):\n",
    "            s = lst[i] + lst[j] + lst[k]\n",
    "            if s <= M:\n",
    "                answer = max(s, answer)\n",
    "\n",
    "print(answer)"
   ]
