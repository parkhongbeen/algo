    "word = input()\n",
    "\n",
    "count = 0\n",
    "i = 0\n",
    "\n",
    "while len(doc) - i >= len(word):\n",
    "    if doc[i:i + len(word)] == word:\n",
    "        count += 1\n",
    "        i += len(word)\n",
    "    else:\n",
    "        i += 1\n",
    "        \n",
    "print(count)"
